meta {
  name: ResetPassword
  type: http
  seq: 8
}

put {
  url: {{base_url}}/auth/reset
  body: json
  auth: inherit
}

body:json {
  {
    "email": {{talent_email}},
    "verificationToken": {{reset_token}},
    "newPassword": {{password}},
    "oldPassword": ""
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
